<!DOCTYPE html>
<html>
 <div class="container px-5">
  <div class="row">
    <div class="col-md-3">

      <%= render 'sideuser', user: @user %>
      <%= render 'sidenew', book: @book %>

　　</div>


<div class="col-md-8 offset-md-1">
<h2>Books</h2>

<table>
  <thead>
    <tr>
      <th>  </th>
      <th>Image</th>
      <th>name</th>
    </tr>
  </thead>
  <tbody>
 <% @users.each do |user| %>
      <tr>
        <td><%= image_tag user.get_profile_image(100,100) %>
        </td>
        <td><%= user.name %></td>
        <td><%= link_to "Show", user_path(user)%></td>
      </tr>
    <% end %>
  </tbody>
</table>


</html>
</div>
</div>
</div>